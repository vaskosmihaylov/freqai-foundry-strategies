{
  "max_open_trades": 5,
  "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8080
  },
  "freqai": {
    "identifier": "quickadapter-xgboost-futures-dryrun-wsl-allpairs",
    "train_period_days": 21,
    "backtest_period_days": 14,
    "live_retrain_hours": 8,
    "fit_live_predictions_candles": 288,
    "data_kitchen_thread_count": 1,
    "optuna_hyperopt": {
      "enabled": false,
      "n_jobs": 1,
      "n_trials": 10,
      "timeout": 900
    },
    "feature_parameters": {
      "include_corr_pairlist": [
        "BTC/USDT:USDT",
        "ETH/USDT:USDT"
      ],
      "include_timeframes": [
        "5m",
        "15m"
      ],
      "include_shifted_candles": 2,
      "indicator_periods_candles": [
        8,
        16
      ],
      "buffer_train_data_candles": 50
    },
    "model_training_parameters": {
      "device": "cuda",
      "n_jobs": 2,
      "verbosity": 0
    }
  }
}
