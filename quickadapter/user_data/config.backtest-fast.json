{
  "max_open_trades": 1,
  "exchange": {
    "pair_whitelist": [
      "BTC/USDT:USDT"
    ]
  },
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],
  "freqai": {
    "identifier": "quickadapter-xgboost-futures-fastcheck",
    "train_period_days": 14,
    "backtest_period_days": 14,
    "fit_live_predictions_candles": 144,
    "data_kitchen_thread_count": 1,
    "optuna_hyperopt": {
      "enabled": false,
      "n_jobs": 1,
      "n_trials": 5,
      "timeout": 600
    },
    "feature_parameters": {
      "include_corr_pairlist": [
        "BTC/USDT:USDT"
      ],
      "include_timeframes": [
        "5m"
      ],
      "include_shifted_candles": 1,
      "indicator_periods_candles": [
        8,
        16
      ],
      "buffer_train_data_candles": 20
    },
    "model_training_parameters": {
      "device": "cpu",
      "n_jobs": 2,
      "verbosity": 0
    }
  }
}
