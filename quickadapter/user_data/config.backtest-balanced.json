{
  "max_open_trades": 5,
  "exchange": {
    "pair_whitelist": [
      "BTC/USDT:USDT",
      "ETH/USDT:USDT",
      "SOL/USDT:USDT",
      "BNB/USDT:USDT",
      "XRP/USDT:USDT"
    ]
  },
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],
  "freqai": {
    "identifier": "quickadapter-xgboost-futures-balanced",
    "train_period_days": 45,
    "backtest_period_days": 14,
    "fit_live_predictions_candles": 432,
    "data_kitchen_thread_count": 2,
    "optuna_hyperopt": {
      "enabled": false,
      "n_jobs": 1,
      "n_trials": 10,
      "timeout": 1200
    },
    "feature_parameters": {
      "include_corr_pairlist": [
        "BTC/USDT:USDT",
        "ETH/USDT:USDT"
      ],
      "include_timeframes": [
        "5m",
        "15m",
        "1h"
      ],
      "include_shifted_candles": 3,
      "indicator_periods_candles": [
        8,
        16,
        32
      ],
      "buffer_train_data_candles": 50
    },
    "model_training_parameters": {
      "device": "cuda",
      "n_jobs": 4,
      "verbosity": 0
    }
  }
}
